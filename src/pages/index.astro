---
import Layout from '../layouts/Layout.astro';
import Hero from '../components/Hero.astro';
import Intro from '../components/Intro.astro';
import ReceptionDetails from '../components/ReceptionDetails.astro';
import RSVP from '../components/RSVP.astro';
import Registry from '../components/Registry.astro';
import Venue from '../components/Venue.astro';
import FAQs from '../components/FAQs.astro';
import Outro from '../components/Outro.astro';

const title = "Karly & JT are getting married!";
---

<Layout title={title}>
	<Hero />
	<Intro />
	<ReceptionDetails />
	<RSVP />
	<Registry />
	<Venue />
	<FAQs />
	<Outro />
</Layout>